# Copy files to RAM: drive
# so that no sluggish floppy disk access
# performance is a problem.
echo "Copying system files to RAM: for faster access."
echo "Please wait awhile ..."
mkdir /ram/bin
cd /bin
cp * /ram/bin
echo "Done copying files."
